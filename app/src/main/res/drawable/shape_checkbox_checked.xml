<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="oval">
            <size
                android:width="20dp"
                android:height="20dp" />
            <corners android:radius="1dp" />

            <stroke
                android:width="2dp"
                android:color="?attr/colorPrimary" />

            <solid android:color="?attr/colorPrimary" />
        </shape>
    </item>

    <item
        android:bottom="0dp"
        android:left="4dp"
        android:right="0dp"
        android:top="1dp">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="45">
            <shape android:shape="line">
                <stroke
                    android:width="2dp"
                    android:color="@color/white" />
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="0dp"
        android:right="10dp"
        android:left="1dp"
        android:top="7dp">
        <rotate
            android:fromDegrees="-135"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="90">
            <shape android:shape="line">
                <stroke
                    android:width="2dp"
                    android:color="@color/white" />
            </shape>
        </rotate>
    </item>
</layer-list>